{
  "name": "redisdemo",
  "version": "1.0.0",
  "description": "redis demo para cadeira Sistemas de Gerencia de Banco de Dados",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run lint && npm run format:check && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "docker:build": "docker build -t redis-demo .",
    "docker:run": "docker-compose up"
  },
  "repository": {
    "type": "git",
    "url": "redisDemo"
  },
  "author": "Lorenzo Ribeiro Varalo",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "openmeteo": "^1.2.0",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
